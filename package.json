    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@prisma/client": "^5.22.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.5.1",
    "fastify-swagger": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.22.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/axios": "0.9.36",
